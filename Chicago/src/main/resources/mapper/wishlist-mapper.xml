<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.sist.dao.mapper.WishlistMapper">
  	
  	<!-- 위시리스트에서 보여줄 리스트 -->
	<select id="List" resultType="wishlistVO" parameterType="String">
		SELECT book_code, book_name, amount, price, img FROM goods,wishlist WHERE book_code= #{book_code}
	</select>
 	
</mapper>